{
  "name": "jatotheson-monorepo",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace personal-site",
    "dev:personal-site": "npm run dev --workspace personal-site",
    "dev:showcase-project": "npm run dev --workspace showcase-project",
    "build": "node scripts/build-all.mjs",
    "deploy": "npm run build && gh-pages -d dist",
    "build:personal-site": "npm run build --workspace personal-site",
    "build:showcase-project": "npm run build --workspace showcase-project"
  },
  "devDependencies": {
    "gh-pages": "^6.3.0"
  }
}
